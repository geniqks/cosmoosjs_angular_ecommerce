{
  "info": {
    "title": "E-commerce API",
    "version": "v1"
  },
  "openapi": "3.1.0",
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "components": {
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "AuthLoginInputSchema": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "googleIdToken": {
            "type": "string"
          },
          "googleClientId": {
            "type": "string"
          }
        },
        "required": [
          "username"
        ]
      },
      "AuthRegisterInputSchema": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "lastname": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "name",
          "lastname",
          "email",
          "password"
        ]
      },
      "AuthResponseSchema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "lastname": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "token": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "username",
          "name",
          "lastname",
          "email",
          "token"
        ]
      }
    },
    "parameters": {}
  },
  "paths": {
    "/auth/login": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthLoginInputSchema"
              }
            }
          }
        },
        "responses": {}
      }
    },
    "/auth/register": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthRegisterInputSchema"
              }
            }
          }
        },
        "responses": {}
      }
    },
    "/user/me": {
      "get": {
        "tags": [
          "User"
        ],
        "responses": {
          "200": {
            "description": "Returns an error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AuthResponseSchema"
                }
              }
            }
          }
        }
      }
    },
    "/": {
      "get": {
        "responses": {}
      }
    }
  }
}